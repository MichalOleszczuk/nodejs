{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch async-class-builder",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "node",
      "runtimeArgs": ["--nolazy", "-r", "ts-node/register/transpile-only" ,"-r", "tsconfig-paths/register"],

      "args": ["src/async-class-builder/index.ts"],
      
      "cwd": "${workspaceRoot}",
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": ["<node_internals>/**", "node_modules/**"],
      "outputCapture": "std"
    },
    {
      "name": "Launch async-vs-generator",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "node",
      "runtimeArgs": ["--nolazy", "-r", "ts-node/register/transpile-only" ,"-r", "tsconfig-paths/register"],

      "args": ["src/async-vs-generator/index.ts"],
      
      "cwd": "${workspaceRoot}",
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": ["<node_internals>/**", "node_modules/**"],
      "outputCapture": "std"
    },
    {
      "name": "Launch bind-call-apply",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "node",
      "runtimeArgs": ["--nolazy", "-r", "ts-node/register/transpile-only" ,"-r", "tsconfig-paths/register"],

      "args": ["src/bind-call-apply/index.ts"],
      
      "cwd": "${workspaceRoot}",
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": ["<node_internals>/**", "node_modules/**"],
      "outputCapture": "std"
    },
    {
      "name": "Launch upload-server",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "node",
      "runtimeArgs": ["--nolazy", "-r", "ts-node/register/transpile-only" ,"-r", "tsconfig-paths/register"],

      "args": ["src/node-server-upload-test-websocket/index.ts"],
      
      "cwd": "${workspaceRoot}",
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": ["<node_internals>/**", "node_modules/**"],
      "outputCapture": "std"
    },
    {
      "name": "Launch singleton",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "node",
      "runtimeArgs": ["--nolazy", "-r", "ts-node/register/transpile-only" ,"-r", "tsconfig-paths/register"],

      "args": ["src/singleton/index.ts"],
      
      "cwd": "${workspaceRoot}",
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": ["<node_internals>/**", "node_modules/**"],
      "outputCapture": "std"
    },
    {
      "name": "Launch singleton-vs-class",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "node",
      "runtimeArgs": ["--nolazy", "-r", "ts-node/register/transpile-only" ,"-r", "tsconfig-paths/register"],

      "args": ["src/singleton-vs-class/index.ts"],
      
      "cwd": "${workspaceRoot}",
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": ["<node_internals>/**", "node_modules/**"],
      "outputCapture": "std"
    },
    {
      "name": "Launch websocket",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "node",
      "runtimeArgs": ["--nolazy", "-r", "ts-node/register/transpile-only" ,"-r", "tsconfig-paths/register"],

      "args": ["src/websocket/index.ts"],
      
      "cwd": "${workspaceRoot}",
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": ["<node_internals>/**", "node_modules/**"],
      "outputCapture": "std"
    },
    {
      "type": "node",
      "name": "Unit testing",
      "request": "launch",
      "args": ["--config", "./test/config/unit/jest.unit.config.ts"],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "program": "${workspaceFolder}/node_modules/jest/bin/jest",
      "outputCapture": "std"
    },
    {
      "type": "node",
      "name": "E2E testing",
      "request": "launch",
      "args": ["--config", "./test/config/e2e/jest.e2e.config.ts"],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "program": "${workspaceFolder}/node_modules/jest/bin/jest",
      "outputCapture": "std"
    },
    {
      "name": "Build",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "node",
      "runtimeArgs": ["--nolazy", "-r", "ts-node/register/transpile-only" ,"-r", "tsconfig-paths/register"],

      "args": ["scripts/build.ts"],
      
      "cwd": "${workspaceRoot}",
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": ["<node_internals>/**", "node_modules/**"],
      "outputCapture": "std"
    }
  ]
}